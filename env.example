# AION Environment Configuration
# Скопируйте этот файл в .env и настройте свои ключи

# Inference.net API Key для AI анализа
INFERENCE_API_KEY=your_inference_api_key_here

# GitHub API Token (опционально, для расширенного анализа)
GITHUB_TOKEN=your_github_token_here

# Настройки веб-интерфейса
WEB_HOST=localhost
WEB_PORT=5000

# Настройки API сервера  
API_HOST=localhost
API_PORT=8000

# Настройки мониторинга
MONITORING_ENABLED=true
MONITORING_INTERVAL=5